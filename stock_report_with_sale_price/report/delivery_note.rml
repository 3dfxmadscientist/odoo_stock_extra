    <section>
      <para style="P20">[[repeatIn(picking.sale_id.order_line,'line')]]</para>
      <blockTable colWidths="204.0,82.0,82.0,81.0,79.0" style="Move_Line_Contect_Assign_State">
        <tr>
          <td>
            <para style="P2">[[ format(line.name) ]] </para>
          </td>
          <td>
            <para style="P15">[[ formatLang(line.product_uos and line.product_uos_qty or line.product_uom_qty) ]] </para>
          </td>
          <td>
            <para style="P15">[[ line.product_uos and line.product_uos.name or line.product_uom.name ]]</para>
          </td>
          <td>
            <para style="P15">[[ formatLang(line.final_price , digits=get_digits(dp='Product Price'))]]</para>
          </td>
          <td>
            <para style="P16">[[ formatLang(line.subtotal_with_tax, digits=get_digits(dp='Account'), currency_obj=picking.sale_id.pricelist_id.currency_id) ]] </para>
          </td>
        </tr>
      </blockTable>
    </section>
    <blockTable colWidths="194.0,82.0,146.0,98.0" repeatRows="1" style="Table5">
      <tr>
        <td>
          <para style="P8">
            <font face="Helvetica" size="9.0">Total :合计 </font>
          </para>
        </td>
        <td>
          <para style="P9">[[ get_total_qty(picking.sale_id) ]]</para>
        </td>
        <td>
          <para style="P4">
            <font color="white"> </font>
          </para>
        </td>
        <td>
          <para style="P10">[[ formatLang(picking.sale_id .amount_total, dp='Account', currency_obj=picking.sale_id .pricelist_id.currency_id) ]]</para>
        </td>
      </tr>
    </blockTable>
    <para style="Standard">
