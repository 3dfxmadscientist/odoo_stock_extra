  <story>
    <para style="terp_default_8">[[repeatIn(objects,'picking')]]</para>
    <para style="terp_default_8">[[picking.type == 'out' and picking.partner_id and setLang(picking.partner_id.lang) or '']]</para>
    <blockTable colWidths="261.0,261.0" style="Table_Address_detail">
      <tr>
        <td>
          <para style="P17">Supplier Address | 供应商地址: [[ (picking.type == 'in' or removeParentNode('para')) and '' ]]</para>
          <para style="P17">Customer Address | 客户地址 : [[ (picking.type == 'out' or removeParentNode('para')) and '' ]]</para>
          <para style="P17">Shipping Address | 送货地址 : [[ (picking.type == 'internal' or removeParentNode('para')) and '' ]]</para>
          <para style="P1">[[ (picking.partner_id and picking.partner_id.id and picking.partner_id.title.name) or '' ]] [[ picking.partner_id and picking.partner_id.id and picking.partner_id.name ]]</para>
          <para style="P1">[[ picking.partner_id and display_address(picking.partner_id) ]]</para>
          <para style="terp_default_9">
            <font color="white"> </font>
          </para>
        </td>
        <td>
          <para style="terp_default_Bold_9">Contact Address<font face="Helvetica"> | 联系人地址</font> :</para>
          <para style="P3">[[ picking.partner_id and picking.partner_id.title.name or '' ]] [[ picking.partner_id and picking.partner_id.name or '' ]]</para>
          <para style="P3">[[ picking.partner_id and display_address(picking.partner_id) ]]</para>
        </td>
      </tr>
    </blockTable>
    <blockTable colWidths="521.0" style="Table_Title_String">
      <tr>
        <td>
          <para style="terp_header"><font face="Helvetica-Bold" size="12.0">Delivery Note | 送货单 : </font>[[ (picking.type == 'internal' or removeParentNode('para')) and '' ]] [[ picking.name ]]</para>
        </td>
      </tr>
    </blockTable>
    <blockTable colWidths="109.0,149.0,156.0,105.0" style="Header_Order_Reference_Tbl">
      <tr>
        <td>
          <para style="P11">SO reference</para>
        </td>
        <td>
          <para style="P11">[[ picking.sale_id.state in ['draft','sent'] and removeParentNode('para') ]] Date Ordered</para>
          <para style="P11">[[ picking.sale_id.state not in ['draft','sent'] and removeParentNode('para') ]] Quotation Date</para>
        </td>
        <td>
          <para style="P11">Salesperson</para>
        </td>
        <td>
          <para style="P11">Payment Term</para>
        </td>
      </tr>
      <tr>
        <td>
          <para style="P12">销售单号</para>
        </td>
        <td>
          <para style="P14">下单日期</para>
        </td>
        <td>
          <para style="P13">销售员</para>
        </td>
        <td>
          <para style="P12">付款条款</para>
        </td>
      </tr>
    </blockTable>
    <blockTable colWidths="109.0,149.0,156.0,105.0" style="Content_Order_Reference_Table">
      <tr>
        <td>
          <para style="terp_default_Centre_8">[[ picking.sale_id.name or '' ]]</para>
        </td>
        <td>
          <para style="terp_default_Centre_8">[[ formatLang(picking.sale_id.date_order,date = True) ]]</para>
        </td>
        <td>
          <para style="terp_default_Centre_8">[[ (picking.sale_id.user_id and picking.sale_id.user_id.name) or '' ]]</para>
        </td>
        <td>
          <para style="terp_default_Centre_8">[[ (picking.sale_id.payment_term and picking.sale_id.payment_term.name) or '' ]]</para>
        </td>
      </tr>
    </blockTable>
    <para style="P18">
      <font color="white"> </font>
    </para>
    <blockTable colWidths="194.0,82.0,81.0,82.0,81.0" repeatRows="1" style="Table2">
      <tr>
        <td>
          <para style="P4">Description</para>
